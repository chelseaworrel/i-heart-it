<div class="row">
  <% current_user.media.each do |h| %>
<div class="col-md-offset-3 col-md-6 col-md-offset-3 card">
<% if h.caption %>
  <span><%= image_tag h.caption.from.profile_picture, class: "img-circle circle" %>&nbsp;&nbsp;<%= h.caption.from.username%></span>
<% end %>
<div class="image">
  <%=image_tag h.images.standard_resolution.url, class: "feed-pics" %><br>
</div>

<div class="likes">
  <%=h.likes['count']%> likes
</div><br>
  <%= h.caption.from.username if h.caption %>
  <div class="comment-text">
    <h5><%= h.caption.text if h.caption %></h5>
      <% h.comments.data.first(2).each do |comment| %>
        <%= comment.from.username %> <h5> <%= comment.text %> </h5>
      <% end %>
  </div>
</div>
<div class="row post-pics"></div>
<% end %>
</div>


<h1>HOMEDECOR</h1>
  <% current_user.homedecor.each do |h|  %>
  <%= image_tag(h.images.standard_resolution.url, size: "30x30") %>
  <% end %>

  <h1>REMODEL</h1>
  <% current_user.remodel.each do |h|  %>
  <%= image_tag(h.images.standard_resolution.url, size: "30x30") %>
  <% end %>
